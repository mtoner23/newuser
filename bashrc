#Obvious
alias resource="source ~/.bashrc"

# Navigation
alias cd..="cd .."
alias ..="cd .."
alias ...="cd ../.."
alias ll='ls -lah'
alias cdg='cd ~/git'

#Git
alias gdom='git diff origin master'
alias gdom='git diff origin main'
alias gdh='git diff HEAD'
alias pull='git pull --rebase'
alias push='git push'


